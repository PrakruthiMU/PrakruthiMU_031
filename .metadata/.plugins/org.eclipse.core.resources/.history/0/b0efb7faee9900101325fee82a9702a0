package com;

public class hallow_rectangle {
public static void hallow_rect_angle(int row, int col) {
	for(int i = 1;i<=row;i++) {
		for(int j=;j<=col;j++) {
			if(i==1||i==row||j==1||j==col) {
				System.out.println("*");
			}
			else {
				System.out.println(" ");
			}
		}
	System.out.println();
}
}
public static void main(String[] args) {
	hallow_rect_angle(4,5);
}
}
