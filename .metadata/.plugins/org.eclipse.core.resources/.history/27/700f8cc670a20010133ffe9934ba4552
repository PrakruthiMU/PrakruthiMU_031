package com;

public class recurssion_fact {
public static void main(String[] args) {
	int numb = 5;
	long factorial = 1;
	if (numb < 0) {
		System.out.println("Factorial defined is a negative number");
	}else if(numb == 0) {
		System.out.println("Factorial of 0 is 1");
	}else {
		for (int i = 1; i <= numb; i++) {
			factorial *= i;
		}
	}System.out.println("Factorial of" +numb+ "is:"+factorial);
}
}
