package Strings;

public class substring {
public static void main(String[] args) {
	String a = "pri";
	String b = "ganu";
	String c="kav";
	a=a+b+c;
	b=a.substring(0,a.length()-b.length());
	a=a.substring(b.length());
	System.out.println("a:"+a);
	System.out.println("b:"+b);
	System.out.println("c:"c);
}}